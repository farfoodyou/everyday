Color	hello.cpp	/^enum Color {$/;"	g	file:
blue	hello.cpp	/^    red, blue, yellow, green$/;"	e	enum:Color	file:
green	hello.cpp	/^    red, blue, yellow, green$/;"	e	enum:Color	file:
main	hello.cpp	/^int main(){$/;"	f
print	hello.cpp	/^void print(Color c){$/;"	f
red	hello.cpp	/^    red, blue, yellow, green$/;"	e	enum:Color	file:
yellow	hello.cpp	/^    red, blue, yellow, green$/;"	e	enum:Color	file:
